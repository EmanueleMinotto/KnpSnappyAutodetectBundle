{
    "authors": [
        {
            "email": "minottoemanuele@gmail.com",
            "name": "Emanuele Minotto"
        }
    ],
    "autoload": {
        "psr-4": {
            "EmanueleMinotto\\KnpSnappyAutodetectBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EmanueleMinotto\\KnpSnappyAutodetectBundle\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "description": "Symfony Bundle for KnpSnappy autodetect",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "keywords": [
        "bundle",
        "knp",
        "snappy",
        "symfony",
        "wkhtmltoimage",
        "wkhtmltopdf"
    ],
    "license": "MIT",
    "name": "emanueleminotto/knp-snappy-autodetect-bundle",
    "require": {
        "php": "^5.5|^7.0",
        "h4cc/wkhtmltoimage-amd64": "^0.12.3",
        "h4cc/wkhtmltoimage-i386": "^0.12.3",
        "h4cc/wkhtmltopdf-amd64": "^0.12.3",
        "h4cc/wkhtmltopdf-i386": "^0.12.3",
        "knplabs/knp-snappy-bundle": "^1.4",
        "messagedigital/wkhtmltoimage-osx": "^0.10.0-RC2",
        "messagedigital/wkhtmltopdf-osx": "^0.9.9",
        "symfony/framework-bundle": "^2.8|^3.0",
        "wemersonjanuario/wkhtmltox-windows-64bit": "0.12.2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0",
        "scrutinizer/ocular": "^1.3",
        "squizlabs/php_codesniffer": "^2.5",
        "symfony/expression-language": "^2.8|^3.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "type": "symfony-bundle"
}
